/dts-v1/;

#include "qcom-ipq4019.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/soc/qcom,tcsr.h>

/ {
	model = "Linksys WHW03 (Velop)";
	compatible = "linksys,whw03", "qcom,ipq4019";

	aliases {
		led-boot = &led_blue;
		led-failsafe = &led_red;
		led-running = &led_green;
		led-upgrade = &led_red;
        serial0 = &blsp1_uart1;
	};


	keys {
		compatible = "gpio-keys";

		button@1 {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&tlmm 18 GPIO_ACTIVE_HIGH>;
		};
	};
	
	// U-boot init=/sbin/init rootfstype=ext4 root=/dev/mmcblk0p15  rootwait console=ttyMSM0,115200n8

	chosen {
		//bootargs = " rootfstype=ext4 root=/dev/mmcblk0p15  rootwait console=ttyMSM0,115200n8";
		bootargs-append = " rootfstype=squashfs clk_ignore_unused";
	};


	soc {
		rng@22000 {
			status = "okay";
		};

		mdio@90000 {
			status = "okay";
		};

		ess-psgmii@98000 {
			status = "okay";
			resets = <&gcc ESS_PSGMII_ARES>;
        	reset-names = "psgmii_rst";
		};

		/*counter@4a1000 {
			compatible = "qcom,qca-gcnt";
			reg = <0x4a1000 0x4>;
		};*/


		tcsr@1949000 {
			compatible = "qcom,tcsr";
			reg = <0x1949000 0x100>;
			qcom,wifi_glb_cfg = <TCSR_WIFI_GLB_CFG>;
		};

		tcsr@194b000 {
			compatible = "qcom,tcsr";
			reg = <0x194b000 0x100>;
			qcom,usb-hsphy-mode-select = <TCSR_USB_HSPHY_HOST_MODE>;
		};

		ess_tcsr@1953000 {
			compatible = "qcom,tcsr";
			reg = <0x1953000 0x1000>;
			qcom,ess-interface-select = <TCSR_ESS_PSGMII>;
		};

		tcsr@1957000 {
			compatible = "qcom,tcsr";
			reg = <0x1957000 0x100>;
			qcom,wifi_noc_memtype_m0_m2 = <TCSR_WIFI_NOC_MEMTYPE_M0_M2>;
		};

		usb2@60f8800 {
			status = "okay";
		};

		usb3@8af8800 {
			status = "okay";
		};

		crypto@8e3a000 {
			status = "okay";
		};

		watchdog@b017000 {
			status = "okay";
		};

		ess-switch@c000000 {
			status = "okay";
		};

		edma@c080000 {
			status = "okay";
		};
	};
};

&tlmm {
	mdio_pins: mdio_pinmux {
		mux_1 {
			pins = "gpio6";
			function = "mdio";
			bias-pull-up;
		};
		mux_2 {
			pins = "gpio7";
			function = "mdc";
			bias-pull-up;
		};
	};

	i2c_0_pins: i2c_0_pinmux {
		pinmux {
			function = "blsp_i2c0";
			pins = "gpio58", "gpio59";
			bias-disable;
		};
	};

	serial_0_pins: serial0_pinmux {
		pins = "gpio16", "gpio17";
		function = "blsp_uart0";
		bias-disable;
	};

	serial_1_pins: serial1_pinmux {
		pins = "gpio8", "gpio9";
		function = "blsp_uart1";
		bias-disable;
	};

	uart_1_pins: uart1_pinmux {
		mux {
			pins = "gpio8", "gpio9", "gpio10", "gpio11";
			function = "blsp_uart1";
			bias-disable;
		};
	};
	
	spi_1_pins: spi_1_pinmux {
		mux {
			pins = "gpio44", "gpio45", "gpio46", "gpio47";
			function = "blsp_spi1";
			bias-disable;
		};
		host_int {
			pins = "gpio42";
			function = "gpio";
			input;
		};
		cs {
			pins = "gpio45";
			function = "gpio";
			bias-pull-up;
		};
		reset {
            pins = "gpio49";
            function = "gpio";
            output-high;
        };
	};
	
	wifi1_pins: wifi1_pinmux {
        mux_1 {
            pins = "gpio37";
            output-high;
            function = "wcss0_dbg16";
            bias-pull-up;
        };

        mux_2 {
            pins = "gpio43";
            function = "wifi_wci1";
            bias-pull-up;
        };

        mux_3 {
            pins = "gpio52";
            output-high;
            function = "gpio";
            bias-pull-up;
        };
    };
    
    sd_pins: sd_0_pinmux {
        mux {
                function = "sdio";
                pins = "gpio23", "gpio24", "gpio25", "gpio26",
                        "gpio27", "gpio28", "gpio29", "gpio30", 
                        "gpio31";
        };

        sd7 {
                function = "sdio";
                pins = "gpio32";
                bias-disable;
        };
    };
};

&mdio {
	status = "ok";
	pinctrl-0 = <&mdio_pins>;
	pinctrl-names = "default";
	//qcom,mdio_offset = <0x18>;
	//qcom,mdio_offset_limit = <0x5>;
	reset-gpios = <&tlmm 19 GPIO_ACTIVE_LOW>;
	//phy-reset-gpio = <&tlmm 19 GPIO_ACTIVE_LOW>;
};

&blsp_dma {
	status = "okay";
};

&blsp1_uart1 {
	status = "okay";
	pinctrl-0 = <&serial_0_pins>;
	pinctrl-names = "default";
};

/*&blsp1_uart2 {
	status = "okay";
	pinctrl-0 = <&serial_1_pins>;
	pinctrl-names = "default";
};*/

&vqmmc {
	status = "okay";
};

&sdhci {
	vqmmc-supply = <&vqmmc>;
	pinctrl-0 = <&sd_pins>;
	pinctrl-names = "default";
	cd-gpios = <&tlmm 22 GPIO_ACTIVE_LOW>;
	sd-ldo-gpios = <&tlmm 33 GPIO_ACTIVE_LOW>;
	status = "okay";
};



&cryptobam {
	status = "okay";
};

&blsp1_spi2 {
    pinctrl-0 = <&spi_1_pins>;
    pinctrl-names = "default";
    status = "okay";
    em358x@0 {
        compatible = "spidev";
        reg = <0x00>;
        spi-max-frequency = <0xb71b00>;
    };
};

&pcie0 {
	status = "okay";

	perst-gpio = <&tlmm 38 GPIO_ACTIVE_LOW>;
	wake-gpio = <&tlmm 40 GPIO_ACTIVE_LOW>;
	clkreq-gpio = <&tlmm 39 GPIO_ACTIVE_LOW>;

	bridge@0,0 {
		reg = <0x00000000 0 0 0 0>;
		#address-cells = <3>;
		#size-cells = <2>;
		ranges;

		wifi2: wifi@1,0 {
			compatible = "qcom,ath10k";
			reg = <0x00010000 0 0 0 0>;
		};
	};
};

&qpic_bam {
	status = "okay";
};


&blsp1_i2c3 {
	pinctrl-0 = <&i2c_0_pins>;
	pinctrl-1 = <&i2c_0_pins>;
	pinctrl-names = "i2c_active", "i2c_sleep";

	status = "okay";

	pca9633@62 {
		compatible = "nxp,pca9633";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <98>;
		nxp,hw-blink;
		led_red: red@0 {
			label = "whw03:red:power";
			reg = <0>;
		};
		led_green: green@1 {
			label = "whw03:green:power";
			reg = <1>;
		};
		led_blue: blue@2 {
			label = "whw03:blue:power";
			reg = <2>;
		};
		led_unusued: unused@3 {
			label = "whw03:unused:power";
			reg = <3>;
		};
	};
};

&usb2_hs_phy {
	status = "okay";
};

&usb3_hs_phy {
	status = "okay";
};

&usb3_ss_phy {
	status = "okay";
};

&wifi0 {
	status = "okay";
	pinctrl-0 = <&wifi1_pins>;
	pinctrl-names = "default";
	qcom,ath10k-calibration-variant = "linksys-whw03-fcc";
};

&wifi1 {
	status = "okay";
	ieee80211-freq-limit = <5170000 5330000>;
	qcom,ath10k-calibration-variant = "linksys-whw03-fcc";
};

&wifi2 {
	status = "okay";
	ieee80211-freq-limit = <5490000 5835000>;
	qcom,ath10k-calibration-variant = "linksys-whw03-fcc";
};
